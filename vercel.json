{
  "version": 2,
  "functions": {
    "server/index.ts": {
      "memory": 2048
    }
  },
  "builds": [
    {
      "src": "client/vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "server/index.ts"
    }
  ]
}
